{
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Ilia Krasnoschekov (kelbek)",
            "email": "dev_kelbek@mail.ru"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.12",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/yaml": "5.2.*",
        "kelbek/model-builder-symfony": "dev-main"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/kolobok-kelbek/model-builder-symfony.git"
        },
        {
            "type": "git",
            "url": "https://github.com/kolobok-kelbek/model-builder.git"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "My\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "My\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
